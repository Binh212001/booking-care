# Application
PORT=3000
NODE_ENV=development

# Database Configuration (PostgreSQL)
# Using port 25432 as mapped in docker-compose.yml (host:container = 25432:5432)
DB_HOST=localhost
DB_PORT=25432
DB_USER=root
DB_PASS=root
DB_NAME=booking-care

# Redis Configuration
# Using port 6379 as mapped in docker-compose.yml
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT Configuration (if needed)
JWT_SECRET=your-secret-key-here
JWT_EXPIRES_IN=7d

# API Configuration
API_PREFIX=api
